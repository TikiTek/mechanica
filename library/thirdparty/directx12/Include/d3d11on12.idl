cpp_quote( "    {" )
cpp_quote( "        *static_cast<CD3D12_RESOURCE_DESC*>(this) = Buffer( o.ByteWidth, D3D12_RESOURCE_MISC_NONE );" )
cpp_quote( "        MiscFlags |= ConvertD3D11Flags( o.BindFlags );" )
cpp_quote( "    }" )
